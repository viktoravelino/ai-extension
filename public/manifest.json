{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "permissions": ["scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "32": "icon32.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
